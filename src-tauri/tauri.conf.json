{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Tauri-Docs",
  "version": "1.0.0",
  "identifier": "io.github.tauri-apps.tauri-docs",
  "build": {
    "beforeDevCommand": "pnpm run dev",
    "devUrl": "http://localhost:4321",
    "beforeBuildCommand": "pnpm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Tauri Docs",
        "maximized":true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
